{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "openai-api-usage2",
  "title": "OpenAI API Usage",
  "description": "Check your OpenAI API usage from the menu bar",
  "icon": "command-icon.png",
  "author": "asaday",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "API Usage",
      "description": "Check your OpenAI API usage",
      "mode": "menu-bar",
      "interval": "10m"
    }
  ],
  "preferences": [
    {
      "name": "openAiSessionKey",
      "title": "OpenAI Session Key",
      "required": true,
      "type": "textfield",
      "description": "OpenAI Session Key",
      "default": ""
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.4.8",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}